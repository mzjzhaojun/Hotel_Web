var mes="";
function getmes(status){
	mes="";
	switch (status) {
	case '1':
		mes='操作成功';
		break;
	case '0':
		mes='帐户格式不正确(正确的格式为:员工编号@企业编号)';
		break;
	case '-1':
		mes='服务器拒绝(速度过快、限时或绑定IP不对等)如遇速度过快可延时再发';
		break;
	case '-2':
		mes='密钥不正确';
		break;
	case '-3':
		mes='密钥已锁定';
		break;
	case '-4':
		mes='参数不正确(内容和号码不能为空，手机号码数过多，发送时间错误等)';
		break;
	case '-5':
		mes='无此帐户';
		break;
	case '-6':
		mes='帐户已锁定或已过期';
		break;
	case '-7':
		mes='帐户未开启接口发送';
		break;
	case '-8':
		mes='不可使用该通道组';
		break;
	case '-9':
		mes='帐户余额不足';
		break;
	case '-10':
		mes='内部错误';
		break;
	case '-11':
		mes='扣费失败';
		break;
	}
}
