<script type="text/javascript">
    ;(function($){
        var ti = $(".tabs_on").index();
        $(".coupon").children().eq(ti).show().siblings().hide();
        $(".tabs_t > div").tap(function(){
            var tindex = $(this).index();
            $(this).addClass('tabs_on').siblings().removeClass('tabs_on');
            $(".coupon").children().eq(tindex).show().siblings().hide();
        })
    })(Zepto);
</script>
</head>
<body class="pa welbg">
    
    <div class="main1">
       <!-- 使用情况 -->
       <div class="tabs">
           <div class="tabs_t clearfix">
               <div class="tabs_on">未使用</div>
               <div>已使用</div>
               <div>已过期</div>
           </div>
           <div class="coupon">
               <!-- 未使用列表 -->
               <ul>
               	<div class="coupon_no">暂无相关数据！</div>                                          
               </ul>

               <!-- 已使用列表 -->
               <ul>
               	<div class="coupon_no">暂无相关数据！</div>                                        
               </ul>

               <!-- 已过期列表 -->
               <div class="coupon_no">暂无相关数据！</div>                        
               <ul></ul>
           </div>
       </div>
   </div>
	
</body>
</html>