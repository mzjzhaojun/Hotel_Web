<script type="text/javascript">
		G = function(objId){return document.getElementById(objId)};
		Gn = function(objName){return document.getElementsByName(objName)};
		window.onload=function(){
			var parameter = "";
			var url = "/Hotel/rest/reservation";
			RemoteUtil.send(url, "GET", parameter, reservationBack);
		};
		
		function reservationBack(data){
			if(data.succeed){
				if(data.result.statusCode == "200"){
					roomData = data.result.successMsg;
					$(".tlist").empty();
					var list = data.result.successMsg;
					var len = list.length;
					for(var i=0;i<len;i++){
						$(".tlist").append($("<li>"+
				            "<div class=\"tlist_t\">"+list[i].name+"</div>"+
				            "<div class=\"tlist_v clearfix\">"+
				                "<div class=\"tlist_d fr\">"+
				                		""+list[i].detail+""+
				                		"<br><br>"+
				                		"<div style=\"text-indent: 2em;\">地址："+list[i].address+"</div>"+
				                		"<div style=\"text-indent: 2em;\">电话："+list[i].tel+"</div>"+
				                	"</div>"+
				            "</div>"+
				            "<div class=\"tlist_a clearfix\">"+
				                "<a href=\"tel:"+list[i].tel+"\"> <div class=\"fr\">拨打电话>></div></a>"+
				           " </div>"+
				         "</li>"));
					}
					
				}
			}
			
		}
		
	</script>

