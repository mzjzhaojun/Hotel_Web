<script type="text/javascript">
(function($){
//注册
$('#submit').click(function(){
	if($("#password").val()=='')
	{
		$(document).layer('密码长度不能小于6位');
		$("#password").focus();
		return false;
	}else
	{
		if($("#password").val().length<6)
		{
			$(document).layer('密码长度不能小于6位');
		    $("#password").focus();
		    return false;
		}
	}
	if($("#verify").val()=='')
	{
		$(document).layer('验证码不能为空');
		$("#password").focus();
		return false;
	}
	if($("#fpassword").val()!=$("#password").val())
	{
		$(document).layer('两次密码不一致');
		return false;
	}
   var data=$("#form").serialize();
   commonAjaxSubmit("/center/register.html",data);
  return false;
});
$(".login_a").delegate('#login_send', 'click', function(event) 
{
	var url="/center/message.html";
	var  vs= $(this);
	var tel=$(".phone").val();
	if(tel=='')
	{
		$(document).layer('手机号码不能为空');
		$(".phone").focus();
		return false;
	}else
	{
		 var re = /^1\d{10}$/
		 if(!re.test(tel))
		 {
			$(document).layer('手机号码格式不正确');
			$(".phone").focus();
			return false;
		 }
		
	}

	$.post(url,{tel:tel},
	function(data){
		if(data=='success')
		{
			$(document).layer('发送成功！');
			 time(vs);
		}else
		{
			$(document).layer(data);
		}
		
	})
	return false;
})
})(jQuery);
</script>
